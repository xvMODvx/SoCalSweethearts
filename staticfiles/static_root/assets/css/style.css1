  @media only screen and (max-width: 479px) {
    .btn {
      padding: 1px 20px;
      font-size: 11px; } }
  .btn span {
    display: inline-block;
    line-height: inherit;
    -webkit-transition: none;
    -o-transition: none;
    transition: none; }
  .btn i {
    display: inline-block;
    line-height: inherit; }
  .btn.left i {
    margin-right: 10px; }
